# Sahaay AI Environment Configuration

# Application
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Voice Processing
STT_SERVICE=hybrid
TTS_SERVICE=hybrid
AUDIO_COMPRESSION_QUALITY=6

# Language Support
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,hi,ta,te,bn

# External APIs
API_SETU_BASE_URL=https://api.setu.co
API_SETU_API_KEY=your_api_key_here

# Cache Configuration
CACHE_TTL_SECONDS=3600
MAX_CACHE_SIZE=1000

# Privacy and Security
SESSION_TIMEOUT_MINUTES=30
MAX_CONVERSATION_TURNS=50
ENABLE_ANALYTICS=false

# Performance
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT_MS=30000

# Testing
PROPERTY_TEST_ITERATIONS=100